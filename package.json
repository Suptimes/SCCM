{
  "name": "blink-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start --web --port 3000",
    "start": "EXPO_NO_TELEMETRY=1 npx expo start",
    "start:web": "EXPO_NO_TELEMETRY=1 npx expo start --web",
    "start:ios": "EXPO_NO_TELEMETRY=1 npx expo start --ios",
    "start:android": "EXPO_NO_TELEMETRY=1 npx expo start --android",
    "build:web": "EXPO_NO_TELEMETRY=1 npx expo export --platform web",
    "build:ios": "EXPO_NO_TELEMETRY=1 eas build --platform ios",
    "build:android": "EXPO_NO_TELEMETRY=1 eas build --platform android",
    "eject": "EXPO_NO_TELEMETRY=1 npx expo eject",
    "setup": "EXPO_NO_TELEMETRY=1 npx expo install",
    "install:bun": "bun install",
    "install:fast": "bun install --no-postinstall",
    "add": "bun add",
    "doctor": "EXPO_NO_TELEMETRY=1 npx expo doctor",
    "upgrade": "EXPO_NO_TELEMETRY=1 npx expo upgrade",
    "lint": "EXPO_NO_TELEMETRY=1 npx expo lint",
    "prebuild": "EXPO_NO_TELEMETRY=1 npx expo prebuild",
    "prebuild:clean": "EXPO_NO_TELEMETRY=1 npx expo prebuild --clean"
  },
  "dependencies": {
    "@blinkdotnew/sdk": "2.3.5",
    "@expo/vector-icons": "^15.0.3",
    "@legendapp/list": "^2.0.16",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tanstack/react-query": "^5.90.0",
    "expo": "~54.0.25",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "3.4.17",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
